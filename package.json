{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "prestart": "npm run build",
    "deploy": "NODE_ENV=production bun scripts/load-env.ts && prisma migrate deploy && npm run build",
    "db:migrate:dev": "bun scripts/load-env.ts && prisma migrate dev",
    "db:migrate:prod": "NODE_ENV=production bun scripts/load-env.ts && prisma migrate deploy",
    "db:studio:dev": "bun scripts/load-env.ts && prisma studio",
    "db:studio:prod": "NODE_ENV=production bun scripts/load-env.ts && prisma studio",
    "db:seed:dev": "bun scripts/load-env.ts && prisma db seed",
    "db:seed:prod": "NODE_ENV=production bun scripts/load-env.ts && prisma db seed",
    "db:seed:csv:dev": "bun scripts/load-env.ts && bun prisma/seedProductsFromCSV.ts",
    "db:seed:csv:prod": "NODE_ENV=production bun scripts/load-env.ts && bun prisma/seedProductsFromCSV.ts",
    "db:seed:terminals:dev": "bun scripts/load-env.ts && bun prisma/seedTerminalsFromCSV.ts",
    "db:seed:terminals:prod": "NODE_ENV=production bun scripts/load-env.ts && bun prisma/seedTerminalsFromCSV.ts",
    "db:backup": "scripts/backup-db.sh"
  },
  "dependencies": {
    "@api/helcimdevdocs": "file:.api/apis/helcimdevdocs",
    "@clerk/clerk-sdk-node": "^5.0.9",
    "@dotenvx/dotenvx": "^1.43.0",
    "@prisma/adapter-pg": "^5.14.0",
    "@prisma/client": "^6.7.0",
    "bcrypt": "^5.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "debug": "~2.6.9",
    "decimal.js": "^10.4.3",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "imap": "^0.8.17",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.1",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.0",
    "pg": "^8.11.5",
    "zod": "^3.23.8",
    "zod-express-middleware": "^1.4.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-flash": "^0.0.40",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "daisyui": "^4.11.1",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
