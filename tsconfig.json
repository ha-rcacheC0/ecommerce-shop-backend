{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "paths": {
      "*": ["./node_modules/*"]
    }
  },
  "include": ["src/**/*", "prisma/**/*", ".api/**/*"]
}
